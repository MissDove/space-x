{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "interface-name": false,
        "ordered-imports": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "jsx-no-lambda": false
    },
    "linterOptions": {
        "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js", "src/**/*.json"]
    }
}
